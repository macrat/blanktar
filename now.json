{
  "github": {
    "silent": true
  },
  "build": {
    "env": {
      "GITHUB_TOKEN": "@blanktar--github-token",
      "INSTAGRAM_TOKEN": "@blanktar--instagram-token",
      "GOOGLE_ANALYTICS": "@blanktar--google-analytics"
    }
  },
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/cgi-bin/search.py",
      "destination": "/search"
    },
    {
      "source": "/works/(.*)",
      "destination": "/works"
    },
    {
      "source": "/photo(/?)",
      "destination": "/photos"
    },
    {
      "source": "/photo/([0-9]{8}/?)",
      "destination": "/photos"
    },
    {
      "source": "/about.html",
      "destination": "/about"
    },
    {
      "source": "/blog/(20[0-9]{2})/",
      "destination": "/blog/$1"
    },
    {
      "source": "/blog/(20[0-9]{2})/(1[0-2]|0[0-9])/",
      "destination": "/blog/$1/$2"
    },
    {
      "source": "/blog/(20[0-9]{2})/(1[0-2]|0[0-9])/(.+).html",
      "destination": "/blog/$1/$2/$3"
    },
    {
      "source": "/blog/index([0-9]+).html",
      "destination": "/blog?offset=$10"
    },
